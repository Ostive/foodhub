{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/foodhub/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/foodhub/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/foodhub",
  "projects": {
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "deliver-service": {
      "type": "application",
      "root": "apps/deliver-service",
      "entryFile": "main",
      "sourceRoot": "apps/deliver-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/deliver-service/tsconfig.app.json"
      }
    },
    "foodhub": {
      "type": "application",
      "root": "apps/foodhub",
      "entryFile": "main",
      "sourceRoot": "apps/foodhub/src",
      "compilerOptions": {
        "tsConfigPath": "apps/foodhub/tsconfig.app.json"
      }
    },
    "key-service": {
      "type": "application",
      "root": "apps/key-service",
      "entryFile": "main",
      "sourceRoot": "apps/key-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/key-service/tsconfig.app.json"
      }
    },
    "order-service": {
      "type": "application",
      "root": "apps/order-service",
      "entryFile": "main",
      "sourceRoot": "apps/order-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-service/tsconfig.app.json"
      }
    },
    "performance-service": {
      "type": "application",
      "root": "apps/performance-service",
      "entryFile": "main",
      "sourceRoot": "apps/performance-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/performance-service/tsconfig.app.json"
      }
    },
    "restaurant-service": {
      "type": "application",
      "root": "apps/restaurant-service",
      "entryFile": "main",
      "sourceRoot": "apps/restaurant-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/restaurant-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  }
}